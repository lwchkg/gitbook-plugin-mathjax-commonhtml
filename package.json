{
  "name": "gitbook-plugin-mathjax-commonhtml",
  "version": "0.0.6",
  "description": "MathJax integration into GitBook using MathJax's CommonHTML output processor, which is processed in the server side.",
  "homepage": "https://github.com/lwchkg/gitbook-plugin-mathjax-commonhtml",
  "bugs": {
    "url": "https://github.com/lwchkg/gitbook-plugin-mathjax-commonhtml/issues",
    "email": "lwchkg@gmail.com"
  },
  "license": "GPL-3.0",
  "contributors": [
    "WC Leung <lwchkg@gmail.com>"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/lwchkg/gitbook-plugin-mathjax-commonhtml"
  },
  "engines": {
    "gitbook": ">=3.0.0"
  },
  "dependencies": {
    "mathjax-node": ">=1.0.0",
    "fs-extra": ">=1.0.0",
    "snyk": "^1.319.0"
  },
  "keywords": [
    "math",
    "latex"
  ],
  "gitbook": {
    "properties": {
      "defaultScaling": {
        "type": "boolean",
        "default": true,
        "description": "Add the default math font scales (for Georgia and Arial). If you are using custom fonts, or have fontsettings plugin disabled, set this property to false."
      }
    }
  },
  "scripts": {
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "snyk": true
}
